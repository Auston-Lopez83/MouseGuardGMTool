@model List<MouseGuardGMTool.Models.TheGuard>

@{
    ViewData["Title"] = "Home Page";
}
<h2>@ViewData["Message"]</h2>

<div class="row">
	
	<div class="col-sm-6 col-md-4">
		
		<div class="thumbnail">
			<h3 style="text-align:center">@Model.Name</h3>
			<div class="caption">
				<table class="table">
					<thead>
						<th>Ability</th>
						<th>Rating</th>
					</thead>
					<tbody>
						<tr>
							<td>Nature(Mouse):</td>
							<td>@Model.Nature</td>
						</tr>
						<tr>
							<td>Will:</td>
							<td>@p.Will</td>
						</tr>
						<tr>
							<td>Health:</td>
							<td>@Model.Health</td>
						</tr>
						<tr>
							<td>Resources</td>
							<td>@Model.Resources</td>
						</tr>
						<tr>
							<td>Circles</td>
							<td>@Model.Circles</td>
						</tr>
						<tr>
								
						</tr>
					</tbody>

				</table>
			</div>
		</div>
	</div>
	<img src="/images/mg-mice.png" height="400px">
</div>